{
    "version": "0.2.0",
    "configurations": [
        {
        "name": "Debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/target/debug/z2p",
        "args": ["a", "inputs/input.txt"],
        "stopAtEntry": false,
        "targetArchitecture": "x86_64",
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "rust-gdb",
        "dap-compilation": "cargo build",
        "dap-compilation-dir": "${workspaceFolder}"
    },
        {
        "name": "Release",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/target/release/z2p",
        "args": ["a", "inputs/input.txt"],
        "stopAtEntry": false,
        "targetArchitecture": "x86_64",
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "rust-gdb",
        "dap-compilation": "cargo build --release",
        "dap-compilation-dir": "${workspaceFolder}"
    }
    ]
}
